        <!--<FieldUri>http://purl.org/dc/terms/title</FieldUri>-->
        <!--<ValidationFunction>-->
        <!--org.eol.validator.functions.ValidationFunctions.checkTermOfFieldURIisUTF8_FieldValidator-->
